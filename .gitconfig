[user]
	email = chenbao@baidu.com
	name = chenbao
[alias]
	st = status
	df = diff
	dfs = diff --staged
	co = checkout
	br = branch
	bra = branch --all
	cm = commit
	cmm = commit -m
	cms = commit --squash
	cma = commit --amend
	cmf = commit --fixup
	logl = log --oneline --graph
	rst = reset
	rss = restore --staged
	rs = restore
	cp = cherry-pick
[core]
	editor = nvim
[pull]
	rebase = true
[push]
	default = simple
[merge]
	tool = diffview
[mergetool]
	prompt = false
	keepBackup = false
